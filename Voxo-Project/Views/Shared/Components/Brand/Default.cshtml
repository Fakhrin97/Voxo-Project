@model List<BrandImgVM>
@if (Model.Count != 0)
{
    <section class="section-b-space">
        <div class="container">
            <div class="row">
                <div class="col">
                    <div class="brand-slider">
                        @foreach (var item in Model)
                        {
                            <div>
                                <div class="brand-image">
                                    <img src="~/assets/images/voxo-brand/@item.ImageUrl" class="img-fluid blur-up lazyload"
                                         alt="brand logo">
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </section>
}
